# FastAPI Backend Environment Variables
# Copy this file to .env and update with your actual values

# API Key for authenticating POST /send requests
# Generate a strong random key for production (e.g., using: openssl rand -hex 32)
API_KEY=your-secret-api-key-change-in-production-12345

# Port (Railway sets this automatically via $PORT, but you can override for local testing)
PORT=8000

# Optional: Log level (INFO, DEBUG, WARNING, ERROR)
LOG_LEVEL=INFO

